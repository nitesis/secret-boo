<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <runtime>
      <!-- gcConcurrent: true = Runs GC concurrently. This is the default.
      false = Does not run GC concurrently. -->
      <gcConcurrent enabled="true" />
      <!-- GCCpuGroup: false = GC does not support multiple CPU groups. This is the default.
      true = GC supports multiple CPU groups, if server garbage collection is enabled. -->
      <GCCpuGroup enabled="true"/>
      <!-- gcServer: false = Does not run server GC. This is the default.
      true = Runs server GC. -->
      <gcServer enabled="false" />
    </runtime>
</configuration>